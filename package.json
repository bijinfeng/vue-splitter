{
  "name": "@pingtou/vue-splitter",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/es/index.js",
  "typings": "./dist/type/index.d.ts",
  "scripts": {
