    "vue": ">=3"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@types/node": "^22.13.10",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "vitepress": "^1.6.3",
    "vue": "^3.5.13",
    "vue-tsc": "^2.2.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
